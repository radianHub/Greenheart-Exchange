<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Partners_Teachers_Interviews__c</fullName>
    <description>Checks to see if the candidate (Teacher Contact) is under the current partner user.</description>
    <externalId>false</externalId>
    <formula>OR(
(
CASESAFEID( Opportunity__r.AccountId ) =  $User.Account_Id__c
),
( 
CASESAFEID( Candidate_Contact__r.AccountId ) =  $User.Account_Id__c  
)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Partners Teachers Interviews</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
